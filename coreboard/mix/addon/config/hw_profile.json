{
	"shared_devices": {
		"base_i2c_ch0": {
			"class": "PRMSoftI2CBus",
			"scl": "PA4",
			"sda": "PA8",
			"freq": 100000
		},
		"dmm_eeprom_iic_ch0": {
			"class": "PRMSoftI2CBus",
			"scl": "PG8",
			"sda": "PC7",
			"freq": 100000
		},
		"dmm_iic_ch0": {
			"class": "PRMSoftI2CBus",
			"scl": "PF9",
			"sda": "PC13",
			"freq": 100000
		},
		"base_eeprom_ch0":{
			"class": "PRMCAT24C256",
			"dev_addr": 80,
			"i2c_bus": "@base_i2c_ch0"
		},
		"dmm_eeprom_ch0":{
			"class": "PRMCAT24C256",
			"dev_addr": 80,
			"i2c_bus": "@dmm_eeprom_iic_ch0"
		}
	},
	"duts": {
		"dut0": {
			"baseboard": {
				"class": "BaseBoard", 
				"base_iic": "@base_i2c_ch0",
				"eeprom": "@base_eeprom_ch0"
			},
			"dmm": {
				"class": "PRM_DMMV20",
				"i2c_bus": "@dmm_iic_ch0",
				"eeprom": "@dmm_eeprom_ch0",
				"ad7175": {
					"spi": "HWSPI",
					"bus": 3,
					"cs": "PE4",
					"io": "PG6",
					"freq": 5000000
				}
			},
			"server": {
				"ip": "169.254.1.32",
				"port": 7801,
				"keepalive": false
			},
			"site": 0
		}
	},
	"network": {
		"base_ip": "169.254.1.32",
		"ip_addr_file": "./boot/ip_addr_conf.log",
		"default_ip": "169.254.1.254"
	}
}